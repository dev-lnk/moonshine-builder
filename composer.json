{
    "name": "moonshine/project-builder",
    "description": "Project builder",
    "keywords": ["moonshine", "builder"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ivan Levchenko",
            "email": "ivan@mail.com",
            "homepage": "https://github.com/dev-lnk"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.8",
        "mockery/mockery": "^1.4.4",
        "phpstan/phpstan": "^1.4.7",
        "orchestra/testbench": "^4.0|^5.0|^6.0|^7.0|^8.0",
        "brianium/paratest": "^6.8"
    },
    "autoload": {
        "psr-4": {
            "MoonShine\\ProjectBuilder\\": "src/",
            "MoonShine\\ProjectBuilder\\Database\\Factories\\": "database/factories/"
        }
    },
    "conflict": {
        "moonshine/moonshine": "<2.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "MoonShine\\ProjectBuilder\\Providers\\MoonShineBuilderProvider"
            ]
        }
    }
}
