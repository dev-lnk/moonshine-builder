{
    "name": "moonshine/project-builder",
    "description": "Project builder for MoonShine",
    "keywords": ["moonshine", "builder"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ivan Levchenko",
            "email": "lkivanmail@gmail.com",
            "homepage": "https://github.com/dev-lnk"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "MoonShine\\ProjectBuilder\\": "src/",
            "MoonShine\\ProjectBuilder\\Database\\Factories\\": "database/factories/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "conflict": {
        "moonshine/moonshine": "<2.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "MoonShine\\ProjectBuilder\\Providers\\MoonShineBuilderProvider"
            ]
        }
    }
}
